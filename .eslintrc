{
        "root": true,
        "parser": "vue-eslint-parser",
        "extends": [
                "plugin:vue/strongly-recommended",
                "eslint:recommended",
                "prettier"
        ],
        "plugins": ["prettier"],
        "rules": {
                "prettier/prettier": "error",
                "vue/multi-word-component-names": "off",
                "semi": ["error", "always"]
        }
}
